def multiplication_table(number):
    ___ = ___  # Initialize the appropriate variable
   

    while ___:  # Complete the while loop condition.
        result = number * multiplier # make sure that all these variables have been initalized. 
        if  result > 25:
            ___  # Enter the action to take if the result > 25
        print(str(number) + "x" + str(multiplier) + "=" + str(result))
        
        ___ += 1 # This step is to increment so loop can continue to run until condition runs false




multiplication_table(3) 
# Should print: 
# 3x1=3 
# 3x2=6 
# 3x3=9 
# 3x4=12 
# 3x5=15